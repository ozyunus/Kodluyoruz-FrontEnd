{
  "name": "English - Turkish Word",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Get English - Turkish word whenever you open a new tab",
  "homepage_url": "https://wordapi.azurewebsites.net/word",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "chrome_url_overrides": {
    "newtab": "src/override/index.html"
  }    
}